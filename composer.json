{
    "name": "satifest/foundation",
    "description": "Shared Foundation for Satifest",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Satifest\\Foundation\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Satifest\\Foundation\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.3",
        "composer/semver": "^1.5 || ^2.0 || ^3.0",
        "illuminate/database": "^7.10 || ^8.0",
        "illuminate/support": "^7.10 || ^8.0",
        "laravie/query-filter": "^1.6",
        "moneyphp/money": "^3.3",
        "ramsey/uuid": "^4.0",
        "spatie/url": "^1.3"
    },
    "require-dev": {
        "orchestra/canvas": "^5.4.2 || ^6.0",
        "orchestra/testbench": "^5.3 || ^6.0",
        "spatie/test-time": "^1.2"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
