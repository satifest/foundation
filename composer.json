{
    "name": "satifest/foundation",
    "description": "Shared Foundation for Satifest",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Satifest\\Foundation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Satifest\\Foundation\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.3",
        "illuminate/database": "^7.0",
        "composer/semver": "^2.0 || ^3.0",
        "moneyphp/money": "^3.3",
        "spatie/url": "^1.3"
    },
    "require-dev": {
        "orchestra/canvas": "^5.4.2",
        "orchestra/testbench": "^5.2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
